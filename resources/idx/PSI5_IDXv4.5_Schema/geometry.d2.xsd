    <xs:element name="EDMDBSplineCurveForm" type="tns:EDMDBSplineCurveForm" />
    <xs:element name="EDMDParabola" nillable="true" type="tns:EDMDParabola" />
    <xs:element name="EDMDCircle" nillable="true" type="tns:EDMDCircle" />
    <xs:element name="EDMDCircle3Point" nillable="true" type="tns:EDMDCircle3Point" />
    <xs:element name="EDMDCircleCenter" nillable="true" type="tns:EDMDCircleCenter" />
    <xs:element name="EDMDPointOnCurveIntersection" nillable="true" type="tns:EDMDPointOnCurveIntersection" />
    <xs:element name="EDMDPointOnCurveDistance" nillable="true" type="tns:EDMDPointOnCurveDistance" />
    <xs:element name="EDMDEllipse" nillable="true" type="tns:EDMDEllipse" />
</xs:schema>